<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://jakarta.ee/xml/ns/jakartaee"
    xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
    version="6.0">
  
  <display-name>HotelManagement</display-name>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  

  <servlet>
    <servlet-name>SignUpServlet</servlet-name>
    <servlet-class>controller.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUpServlet</servlet-name>
    <url-pattern>/SignUpServlet</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>controller.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
   </servlet-mapping>
   
    <servlet>
        <servlet-name>CreateAgentServlet</servlet-name>
        <servlet-class>controller.CreateAgentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAgentServlet</servlet-name>
        <url-pattern>/CreateAgentServlet</url-pattern>
   </servlet-mapping>
   
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
   </servlet-mapping>
   
     <servlet>
        <servlet-name>DeleteAgentServlet</servlet-name>
        <servlet-class>controller.DeleteAgentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteAgentServlet</servlet-name>
        <url-pattern>/DeleteAgentServlet</url-pattern>
   </servlet-mapping>
   
    <servlet>
        <servlet-name>VisiteurServlet</servlet-name>
        <servlet-class>controller.VisiteurServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisiteurServlet</servlet-name>
        <url-pattern>/VisiteurServlet</url-pattern>
   </servlet-mapping>
    
   
    <servlet>
    <servlet-name>AgentServlet</servlet-name>
    <servlet-class>controller.AgentServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>AgentServlet</servlet-name>
    <url-pattern>/AgentServlet</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>RoomServlet</servlet-name>
    <servlet-class>controller.RoomServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RoomServlet</servlet-name>
    <url-pattern>/RoomServlet</url-pattern>
</servlet-mapping>



<servlet>
    <servlet-name>RoomHotelServlet</servlet-name>
    <servlet-class>controller.RoomHotelServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>RoomHotelServlet</servlet-name>
    <url-pattern>/RoomHotelServlet</url-pattern>
</servlet-mapping>


    
    

</web-app>
